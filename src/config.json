{
  "database": {
    "constring": "postgres://postgres:postgres@127.0.0.1:5432/discord"
  },
  "server": {
    "body_limit": "5mb",
    "port": 3000,
    "cache": {
      "lifespan": 90000
    },
    "ws": {
      "heartbeat_interval": 10000
    }
  },
  "validity": {
    "usernames": {
      "minLength": 1,
      "maxLength": 30,
      "validCharacters": "[a-z0-9_\\-\\.]"
    },

    "guildName": {
      "minLength": 1,
      "maxLength": 30
    },
    
    "messages": {
      "maxLength": 2000
    }
  },
  "syrenity+": [
    33, 139
  ],
  "proxy": {
    "enabled": true,
    "requiresAuthentication": false,
    "requiresSyrenity+": false
  }
}