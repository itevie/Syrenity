{
    "name": "Syrenity Development Codespace",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspace",
    "features": {
        "ghcr.io/itsmechlark/features/postgresql:1": {},
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint",
                "christian-kohler.npm-intellisense"
            ]
        },
        "codespaces": {
            "repositories": {
                "itevie/syrenity-client": {
                    "permissions": {
                        "contents": "write",
                        "pull_requests": "write"
                    }
                },
                "itevie/syrenity-api-client": {
                    "permissions": {
                        "contents": "write",
                        "pull_requests": "write"
                    }
                }
            }
        }
    }
}